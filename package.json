{
  "name": "@npm-publish/test-skeleton-compile",
  "version": "0.1.0",
  "description": "test",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "files": [
    "lib/**/*"
  ],
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "format": "prettier --write src/**/*.{js,jsx,ts,tsx}",
    "lint": "eslint --fix src/**/*.{js,jsx,ts,tsx}",
    "prepare": "yarn rimraf ./lib && yarn build",
    "preversion": "yarn lint",
    "version": "yarn format && git add .",
    "postversion": "git push && git push --tags",
    "test": "jest --no-watchman",
    "coveralls": "jest --coverage && cat ./coverage/lcov.info | coveralls"
  },
  "jest": {
    "preset": "react-native",
    "setupFiles": [
      "./setup-test.js"
    ],
    "moduleFileExtensions": [
      "js",
      "jsx",
      "json",
      "ts",
      "tsx"
    ],
    "transform": {
      "^.+\\.(js|jsx|ts|tsx)$": "babel-jest"
    },
    "testMatch": [
      "**/*.test.ts?(x)"
    ],
    "coveragePathIgnorePatterns": [
      "./src/__tests__/"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jhonnysc/react-native-skeleton-loader"
  },
  "keywords": [
    "node",
    "npm"
  ],
  "bugs": {
    "url": "https://github.com/jhonnysc/react-native-skeleton-loader/issues"
  },
  "homepage": "https://github.com/jhonnysc/react-native-skeleton-loader#readme",
  "dependencies": {
    "react-native-linear-gradient": "^2.5.6",
    "react-native-reanimated": "^2.0.0"
  },
  "devDependencies": {
    "@babel/preset-typescript": "^7.10.1",
    "@types/jest": "^24.0.18",
    "@types/react": "^16.9.5",
    "@types/react-native": "^0.60.19",
    "@types/react-test-renderer": "^16.9.2",
    "@types/styled-components-react-native": "^5.1.1",
    "babel-jest": "^25.5.1",
    "babel-preset-expo": "^8.2.1",
    "coveralls": "^3.1.0",
    "eslint": "^7.2.0",
    "eslint-config-airbnb-typescript-prettier": "^2.1.1",
    "jest": "^24.9.0",
    "prettier": "^1.18.2",
    "react": "^16.10.2",
    "react-native": "^0.61.2",
    "react-native-gesture-handler": "^1.7.0",
    "react-native-linear-gradient": "^2.5.6",
    "react-test-renderer": "^16.13.1",
    "rimraf": "^3.0.2",
    "typescript": "^3.7.2"
  },
  "author": "Jhonny Coliado",
  "license": "MIT"
}
